    "casual": "^1.5.19",
    "compression": "^1.7.2",
    "concurrently": "^3.5.1",
    "connect-mongo": "^2.0.1",
    "cors": "^2.8.4",
    "dotenv": "^5.0.1",
    "express": "^4.16.3",
    "express-graphql": "^0.6.12",
    "graphql": "^0.13.2",
    "graphql-tools": "^3.0.0",
    "mongoose": "^5.0.17",
    "node-fetch": "^2.1.2",
    "nodemon": "^1.17.4",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0"
  },
  "devDependencies": {
